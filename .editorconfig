# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{js}]
charset = utf-8
indent_style = space
indent_size = 4

[{package.json}]
indent_style = space
indent_size = 4
